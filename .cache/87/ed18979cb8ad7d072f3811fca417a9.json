s(pets) {\n  const results = document.querySelector(\"#results\");\n\n  // Clear first\n  results.innerHTML = \"\";\n  // Loops through pets\n  pets.map(pet => {\n    console.log(pet);\n    const div = document.createElement(\"div\");\n    div.classList.add(\"card\", \"card-body\", \"mb-3\");\n    div.innerHTML = `\n        <div class=\"row\">\n        <div class=\"col-sm-6 text-center\">\n               <img class=\"img-fluid rounded mt-2\" src=\"${pet.media.photos\n                 .photo[3].$t\n                 ? `${pet.media.photos.photo[3].$t}`\n                 : `Photo unavailable`}\">\n            </div>\n            <div class=\"col-sm-6\">\n            <h4>${pet.name.$t} (${pet.age.$t})</h4>\n            ${pet.breeds.breed.$t\n              ? `<p class=\"text-secondary\">${pet.breeds.breed.$t}</p>`\n              : `<p class=\"text-secondary\">Breed unavailable</p>`}\n            <ul class=\"list-group\">\n            <li class=\"list-group-item\">${pet.contact.address1.$t\n              ? `<li class=\"list-group-item\">${pet.contact.address1.$t}</li>`\n              : `<li class=\"list-group-item\">Address unavailable</li>`} \n              <li class=\"list-group-item\">${pet.contact.city.$t} ${pet.contact\n      .state.$t} ${pet.contact.zip.$t}</li>\n                \n                    ${pet.contact.phone.$t\n                      ? `<li class=\"list-group-item\">Phone:${pet.contact.phone\n                          .$t}</li>`\n                      : `<li class=\"list-group-item\">Phone no. unavailable</li>`}\n                      ${pet.contact.email.$t\n                        ? `<li class=\"list-group-item\">Email: ${pet.contact\n                            .email.$t}</li>`\n                        : `<li class=\"list-group-item\">Email unavailable</li>`}\n                        <li class=\"list-group-item\">Shelter ID: ${pet.shelterId\n                          .$t}</li>\n                </ul>\n            </div>\n            \n        </div>\n    `;\n    results.appendChild(div);\n  });\n}\n"},"lineCount":null}},"hash":"bf7000a49fbaf4ecd9172e745c98136f","cacheData":{"env":{}}}